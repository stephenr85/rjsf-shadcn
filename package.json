{
  "name": "@rushing/rjsf-shadcn",
  "version": "0.1.0",
  "description": "React JSON Schema Form theme and widgets for shadcn/ui",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./theme": {
      "types": "./dist/theme.d.ts",
      "import": "./dist/theme.mjs",
      "require": "./dist/theme.js"
    },
    "./medical": {
      "types": "./dist/medical/index.d.ts",
      "import": "./dist/medical/index.mjs",
      "require": "./dist/medical/index.js"
    },
    "./model-config": {
      "types": "./dist/model-config/index.d.ts",
      "import": "./dist/model-config/index.mjs",
      "require": "./dist/model-config/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest"
  },
  "keywords": [
    "react",
    "json-schema",
    "form",
    "shadcn",
    "tailwind",
    "rjsf",
    "widgets",
    "medical",
    "healthcare"
  ],
  "author": "Stephen R",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stephenr85/rjsf-shadcn.git"
  },
  "peerDependencies": {
    "@rjsf/core": "^5.24.0",
    "@rjsf/utils": "^5.24.0",
    "@rjsf/validator-ajv8": "^5.24.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "lucide-react": "^0.400.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.6.0"
  }
}